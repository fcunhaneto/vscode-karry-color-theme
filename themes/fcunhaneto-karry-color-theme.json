			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d06fdd"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Tag Name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#9a09b2"
			}
		},
		{
			"name": "CSS: Selectors Meta",
